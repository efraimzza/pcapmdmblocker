<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->
    <!-- https://material.io/blog/android-dark-theme-tutorial -->
    <style name="Base.AppTheme" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:colorPrimary">@color/colorPrimary</item>
        <item name="android:colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:colorAccent">@color/colorAccent</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
    </style>
    
    <!-- Make the Widget.MaterialComponents.Button.TextButton of AlertDialog use the accent color -->
    <style name="AppAlertDialogTheme" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorPrimary">@color/colorAccent</item>
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>

    <!-- Make the TextInputLayout of AlertDialog use the accent color -->
    <style name="TextInputLayoutOutlined" parent="@android:style/Theme.Material.Light.DarkActionBar">
        
    </style>
    <style name="TextInputLayoutOutlinedDropdown" parent="@android:style/Theme.Material.Light.DarkActionBar">
       
    </style>
    <style name="Theme.TextInputLayoutOutlined" parent="">
        <item name="android:colorPrimary">@color/colorAccent</item>
    </style>

    <!-- White app theme. See values-night-v8 for the dark customizations -->
    <style name="AppTheme" parent="Base.AppTheme"/>
    <style name="AppThemeReg" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorPrimary">@color/colorPrimary</item>
        <item name="android:colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:colorAccent">@color/colorAccent</item>
        <item name="android:navigationBarColor">?android:colorPrimary</item>
	</style>
    <style name="AppTheme.NoActionBar" parent="AppTheme">
        
    </style>

    <style name="AppTheme.SplashScreen" parent="AppTheme.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:background">@color/colorActionBar</item>
        <item name="android:elevation">0dp</item>
        <!--<item name="backgroundStacked">@color/colorActionBar</item>-->
        <!--<item name="backgroundSplit">@color/colorActionBar</item>-->
